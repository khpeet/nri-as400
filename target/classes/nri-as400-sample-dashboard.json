{
    "title": "IBM iSeries (AS400) Overview",
    "icon": "bar-chart",
    "grid_column_count": 3,
    "filter": {
        "key_names": ["host", "job", "jobNumber", "severity", "queue", "jobName"],
        "event_types": ["AS400:MessageQueueEvent", "AS400:JobList", "AS400:SystemStatusEvent"],
        "filter_first": false
    },
    "widgets": [{
        "title": "iSeries Servers",
        "nrql": "FROM `AS400:MessageQueueEvent` SELECT uniquecount(host) FACET host SINCE 1 week ago",
        "process_as": "facet_table",
        "width": 1,
        "height": 1,
        "row": 1,
        "column": 1,
        "event_types": ["AS400:MessageQueueEvent"],
        "facet": "host",
        "customizations": {},
        "notes": null
    }, {
        "title": "iSeries Queues",
        "nrql": "FROM `AS400:MessageQueueEvent` SELECT count(message) FACET queue",
        "process_as": "facet_table",
        "width": 1,
        "height": 1,
        "row": 1,
        "column": 2,
        "event_types": ["AS400:MessageQueueEvent"],
        "facet": "queue",
        "customizations": {},
        "notes": null
    }, {
        "title": "Number of Messages by Severity",
        "nrql": "from `AS400:MessageQueueEvent` SELECT count(message) FACET severity",
        "process_as": "facet_bar_chart",
        "width": 1,
        "height": 1,
        "row": 1,
        "column": 3,
        "event_types": ["AS400:MessageQueueEvent"],
        "facet": "severity",
        "customizations": {},
        "notes": null
    }, {
        "title": "Jobs",
        "nrql": "FROM `AS400:MessageQueueEvent` SELECT count(message) FACET job SINCE 1 day ago",
        "process_as": "facet_table",
        "width": 1,
        "height": 2,
        "row": 2,
        "column": 1,
        "event_types": ["AS400:MessageQueueEvent"],
        "facet": "job",
        "customizations": {},
        "notes": null
    }, {
        "title": "Average Number of Batch Jobs Last Minute",
        "nrql": "from `AS400:SystemStatusEvent` SELECT average(numberOfBatchJobsRunning) as 'Number of Batch Jobs' SINCE 1 minutes ago",
        "process_as": "billboard",
        "width": 1,
        "height": 1,
        "row": 2,
        "column": 2,
        "event_types": ["AS400:SystemStatusEvent"],
        "facet": null,
        "customizations": {},
        "notes": null
    }, {
        "title": "Count of Messages by Job Number",
        "nrql": "from `AS400:MessageQueueEvent` SELECT count(message) FACET jobNumber",
        "process_as": "facet_bar_chart",
        "width": 1,
        "height": 1,
        "row": 2,
        "column": 3,
        "event_types": ["AS400:MessageQueueEvent"],
        "facet": "jobNumber",
        "customizations": {},
        "notes": null
    }, {
        "title": "Average Jobs Held on Queue last Minute",
        "nrql": "from `AS400:SystemStatusEvent` SELECT average(numberOfBatchJobsHeldOnQueue) as 'Jobs Held' since 1 minutes ago",
        "process_as": "billboard",
        "width": 1,
        "height": 1,
        "row": 3,
        "column": 2,
        "event_types": ["AS400:SystemStatusEvent"],
        "facet": null,
        "customizations": {},
        "notes": null
    }, {
        "title": "Average Number of Active Jobs Last Minute",
        "nrql": "from `AS400:SystemStatusEvent` select average(numberActiveJobsInSystem) as 'Active Jobs' since 1 minutes ago",
        "process_as": "billboard",
        "width": 1,
        "height": 1,
        "row": 3,
        "column": 3,
        "event_types": ["AS400:SystemStatusEvent"],
        "facet": null,
        "customizations": {},
        "notes": null
    }, {
        "title": "Messages",
        "nrql": "FROM `AS400:MessageQueueEvent` select messageID, message, queue, severity limit 1000",
        "process_as": "event_table",
        "width": 3,
        "height": 1,
        "row": 4,
        "column": 1,
        "event_types": ["AS400:MessageQueueEvent"],
        "facet": null,
        "customizations": null,
        "notes": null
    }, {
        "title": "Processor utilization",
        "nrql": "from `AS400:SystemStatusEvent` select max(percentProcessingUnitUsed) as 'Proc units used', max(percentSharedProcessorPoolUsed) as 'Shared proc pool usage' since 1 minute ago",
        "process_as": "attribute_sheet",
        "width": 1,
        "height": 1,
        "row": 5,
        "column": 1,
        "event_types": ["AS400:SystemStatusEvent"],
        "facet": null,
        "customizations": null,
        "notes": null
    }, {
        "title": "Storage utilization",
        "nrql": "from `AS400:SystemStatusEvent` select max(mainStorageSize) as 'Storage', max(maxUnprotectedStorageUsed) as 'Storage used', max(totalAuxiliaryStorage) as 'ASP', max(percentSystemASPUsed) as '% ASP used' since 1 minute ago",
        "process_as": "attribute_sheet",
        "width": 2,
        "height": 1,
        "row": 5,
        "column": 2,
        "event_types": ["AS400:SystemStatusEvent"],
        "facet": null,
        "customizations": null,
        "notes": null
    }, {
        "title": "",
        "nrql": "SELECT count(*) FROM `AS400:JobList` FACET jobName",
        "process_as": "facet_bar_chart",
        "width": 1,
        "height": 3,
        "row": 6,
        "column": 1,
        "event_types": ["AS400:JobList"],
        "facet": "jobName",
        "customizations": {},
        "notes": null
    }, {
        "title": "Job List Data",
        "nrql": "SELECT jobStatus,jobCPUUsed,jobNumber,jobQueue, jobSubsystem,jobUser  FROM `AS400:JobList`",
        "process_as": "event_table",
        "width": 2,
        "height": 2,
        "row": 6,
        "column": 2,
        "event_types": ["AS400:JobList"],
        "facet": null,
        "customizations": null,
        "notes": null
    }, {
        "title": "",
        "nrql": "SELECT count(*) FROM `AS400:JobList` FACET jobStatus",
        "process_as": "facet_pie_chart",
        "width": 2,
        "height": 1,
        "row": 8,
        "column": 2,
        "event_types": ["AS400:JobList"],
        "facet": "jobStatus",
        "customizations": {},
        "notes": null
    }, {
        "title": "Job counts by subsystem",
        "nrql": "SELECT count(jobName) as 'Jobs' FROM `AS400:JobList` facet jobSubsystem",
        "process_as": "facet_pie_chart",
        "width": 1,
        "height": 1,
        "row": 9,
        "column": 1,
        "event_types": ["AS400:JobList"],
        "facet": "jobSubsystem",
        "customizations": {},
        "notes": null
    }]
}
